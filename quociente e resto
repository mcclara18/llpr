#include <stdio.h>

int main() {
    float n1, n2, quociente, resto;

    printf("Digite o primeiro número inteiro: ");
    scanf("%d", &n1);

    printf("Digite o segundo número: ");
    scanf("%d", &n2);

    quociente = n1 / n2;
    resto = n1 % n2;

    printf("Quociente: %d\n", quociente);
    printf("Resto: %d\n", resto);

    return 0;
}
