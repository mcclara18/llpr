#include <stdio.h>

int main() {
    float num1, num2, num3,soma, resultado;

    printf("Digite o primeiro número: ");
    scanf("%f", &num1);

    printf("Digite o segundo número: ");
    scanf("%f", &num2);

    soma = num1 + num2;

    printf("Digite o terceiro número: ");
    scanf("%f", &num3);

    resultado = soma - num3;

    printf("O resultado da subtração é: %f\n", resultado);

    return 0;
}
